# String concatination

#variables
first_name = "Vinod"
last_name = "Kabadi"

#example code 1
full_name = first_name + " " + last_name
print(full_name)

#example code 2
full_name = "Vinod" + " Kabadi"
print (full_name)

#example code 3
full_name = "Vinod " + "Kabadi"
print (full_name)

#example code 4
str_name = "123"
print(str_name,"1")
#or
print(str_name + "1")

#example code 5
print(first_name + " " + last_name+"!")
